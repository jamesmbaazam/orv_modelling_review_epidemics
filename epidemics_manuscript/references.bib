@Article{Teng2018,
  author        = {Teng, Yue and Bi, Dehua and Guo, Xiaocan and Hu, Dingwen and Feng, Dan and Tong, Yigang},
  journal       = {Journal of Infection},
  title         = {{Contact reductions from live poultry market closures limit the epidemic of human infections with H7N9 influenza}},
  year          = {2018},
  issn          = {15322742},
