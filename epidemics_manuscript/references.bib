  keywords      = {Continuous-time Markov chain, Drug resistance, Final size, Influenza epidemic, xiaoxi},
  language      = {eng},
  mendeley-tags = {xiaoxi},
  pmid          = {17582443},
  url           = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3249396/pdf/nihms29650.pdf},
