  file          = {:C\:/Users/JAMESAZAM/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chen et al. - 2014 - Behavioural response in educated young adults towards influenza A(H1N1)pdm09.pdf:pdf},
  keywords      = {A(H1N1)pdm09, James, contact behaviour, contact matrix, indoor transmission, influenza, modelling, reviewed},
  language      = {English},
