@Article{Werkman2016,
  author    = {Werkman, Marleen and Tildesley, Michael J. and Brooks-Pollock, Ellen and Keeling, Matt J.},
  journal   = {Epidemics},
  title     = {{Preserving privacy whilst maintaining robust epidemiological predictions}},
  year      = {2016},
  issn      = {1878-0067},
  pages     = {35--41},
  volume    = {17},
  abstract  = {Mathematical models are invaluable tools for quantifying potential epidemics and devising optimal control strategies in case of an outbreak. State-of-the-art models increasingly require detailed individual farm-based and sensitive data, which may not be available due to either lack of capacity for data collection or privacy concerns. However, in many situations, aggregated data are available for use. In this study, we systematically investigate the accuracy of predictions made by mathematical models initialised with varying data aggregations, using the UK 2001 Foot-and-Mouth Disease Epidemic as a case study. We consider the scenario when the only data available are aggregated into spatial grid cells, and develop a metapopulation model where individual farms in a single subpopulation are assumed to behave uniformly and transmit randomly. We also adapt this standard metapopulation model to capture heterogeneity in farm size and composition, using farm census data. Our results show that homogeneous models based on aggregated data overestimate final epidemic size but can perform well for predicting spatial spread. Recognising heterogeneity in farm sizes improves predictions of the final epidemic size, identifying risk areas, determining the likelihood of epidemic take-off and identifying the optimal control strategy. In conclusion, in cases where individual farm-based data are not available, models can still generate meaningful predictions, although care must be taken in their interpretation and use.},
  doi       = {10.1016/j.epidem.2016.10.004},
  file      = {:C$backslash$:/Users/Azam/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Werkman et al. - 2016 - Preserving privacy whilst maintaining robust epidemiological predictions.pdf},
  keywords  = {Metapopulation, Simulations, Spatial aggregation, Stochastic model},
  publisher = {Elsevier B.V.},
}

@Article{Doherty2016,
  author    = {Doherty, Mark and Buchy, Philippe and Standaert, Baudouin and Giaquinto, Carlo and Prado-Cohrs, David},
  journal   = {Vaccine},
  title     = {{Vaccine impact: Benefits for human health}},
  year      = {2016},
  issn      = {1873-2518},
  number    = {52},
  pages     = {6707--6714},
  volume    = {34},
  abstract  = {Unlike most drugs, whose benefit is restricted to the individual who takes the drug, prophylactic vaccines have the potential for far-reaching effects that encompass health service utilisation, general health and wellbeing, cognitive development and, ultimately, economic productivity. The impact of immunisation is measured by evaluating effects directly on the vaccinated individual, indirectly on the unvaccinated community (herd protection), the epidemiology of the pathogen (such as changing circulating serotypes or prevention of epidemic cycles), and the additional benefits arising from improved health. Aside from protection of the individual, the broader success of immunisation is dependent on achieving a level of coverage sufficient to interrupt transmission of the pathogen. When evaluating the cost-effectiveness of vaccines, all of these potential benefits need to be accounted for. In many countries where immunisation programmes have been highly successful, the control of disease has meant that the benefits of immunisation have become less obvious. Once a well-known and much-feared disease appears to have disappeared, individuals, including healthcare professionals, no longer view ongoing prevention with the same sense of urgency. Reduced coverage is inevitably associated with resurgence in disease, with outbreaks potentially leading to significant morbidity and loss of life. Ensuring the continued success of immunisation programmes is the responsibility of all: individuals, healthcare professionals, government and industry.},
  doi       = {10.1016/j.vaccine.2016.10.025},
  file      = {:C$backslash$:/Users/Azam/Dropbox/Sacema/Academic/{_}{_}Research/Literature/{_}Current Work/Vaccine {&} Vaccination - Development, Technologies, Efficacy {&} More/Vaccine impact - Benefits for human health.pdf},
  isbn      = {0264-410X},
  keywords  = {Cost, Effectiveness, Health economics, Herd protection, Impact, Vaccine},
  pmid      = {27773475},
  publisher = {The Authors},
}

