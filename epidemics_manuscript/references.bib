@Article{Bhattacharyya2015,
  author   = {Bhattacharyya, Samit and Gesteland, Per H. and Korgenski, Kent and Bj{\o}rnstad, Ottar N. and Adler, Frederick R.},
  journal  = {Proceedings of the National Academy of Sciences of the United States of America},
  title    = {{Cross-immunity between strains explains the dynamical pattern of paramyxoviruses}},
  year     = {2015},
  issn     = {10916490},
  number   = {43},
  pages    = {13396--13400},
  volume   = {112},
  abstract = {Viral respiratory tract diseases pose serious public health problems. Our ability to predict and thus, be able to prepare for outbreaks is strained by the complex factors driving the prevalence and severity of these diseases. The abundance of diseases and transmission dynamics of strains are not only affected by external factors, such as weather, but also driven by interactions among viruses mediated by human behavior and immunity. To untangle the complex out-ofphase annual and biennial pattern of three common paramyxoviruses, Respiratory Syncytial Virus (RSV), Human Parainfluenza Virus (HPIV), and Human Metapneumovirus (hMPV), we adopt a theoretical approach that integrates ecological and immunological mechanisms of disease interactions. By estimating parameters from multiyear time series of laboratory-confirmed cases from the intermountain west region of the United States and using statistical inference, we show that models of immune-mediated interactions better explain the data than those based on ecological competition by convalescence. The strength of cross-protective immunity among viruses is correlated with their genetic distance in the phylogenetic tree of the paramyxovirus family.},
  doi      = {10.1073/pnas.1516698112},
  file     = {:C\:/Users/James Azam/Downloads/Documents/Cross-immunity between strains explains the dynamical pattern of paramyxoviruses.pdf:pdf},
  keywords = {Cross-immunity, Human parainfluenza, Paramyxoviruses, Pathogen interactions, Respiratory syncytial Viruses},
  pmid     = {26460003},
}

