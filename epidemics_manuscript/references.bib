  keywords      = {Activity-driven network, Compartmental model, Contact tracing, Ebola, Epidemic model, James, reviewed},
  mendeley-tags = {James,reviewed},
  pmid          = {30380305},
}

@Article{Newton1992,
  author        = {Newton, E. A.C. and Reiter, P.},
