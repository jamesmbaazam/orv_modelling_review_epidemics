  keywords  = {Expanded program of immunization, Immunization, Immunization activity, Measles, Outbreak response immunization, Supplemental, Vaccine coverage},
  pmid      = {24559281},
  publisher = {BMC Public Health},
  url       = {BMC Public Health},
}

