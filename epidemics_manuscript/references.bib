@Article{ParentduChatelet2001,
  author        = {{Parent du Ch{\^{a}}telet}, Isabelle and Gessner, Bradford D. and {Da Silva}, Alfred},
  journal       = {Vaccine},
  title         = {{Comparison of cost-effectiveness of preventive and reactive mass immunization campaigns against meningococcal meningitis in West Africa: A theoretical modeling analysis}},
  year          = {2001},
