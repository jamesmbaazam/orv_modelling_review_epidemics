@Article{Shahtori2018,
  author        = {Shahtori, Narges Montazeri and Ferdousi, Tanvir and Scoglio, Caterina and Sahneh, Faryad Darabi},
  journal       = {Mathematical Biosciences and Engineering},
  title         = {{Quantifying the impact of early-stage contact tracing on controlling ebola diffusion}},
