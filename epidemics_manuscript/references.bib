  keywords      = {James, athematical model, influenza, reviewed, social distancing},
  language      = {eng},
  mendeley-tags = {James,reviewed},
  pmid          = {25878302},
  url           = {https://watermark.silverchair.com/civ086.pdf?token=AQECAHi208BE49Ooan9kkhW_Ercy7Dm3ZL_9Cf3qfKAc485ysgAAApcwggKTBgkqhkiG9w0BBwagggKEMIICgAIBADCCAnkGCSqGSIb3DQEHATAeBglghkgBZQMEAS4wEQQMs_2nrhFwBykNLAa6AgEQgIICSica_n-XsrliWE2Y23JWt6nxg9PEk7l9jGffr_f-3zGF0MZz},
