}

@Article{Modnak2014,
  author        = {Modnak, Chairat and Wang, Jin and Mukandavire, Zindoga},
  journal       = {International Journal of Biomathematics},
  title         = {{Simulating optimal vaccination times during cholera outbreaks}},
  year          = {2014},
  issn          = {17937159},
  month         = {mar},
  number        = {2},
  volume        = {7},
  abstract      = {The use of cholera vaccines has been increasingly recognized as an effective control measure in cholera endemic countries. However, guidelines for using vaccination during cholera outbreaks are still to be established, and it remains an open question as to how and when the vaccines should be deployed to best control ongoing cholera outbreaks. Here we formulate a new optimal control model to assess the value of cholera vaccines in epidemic settings and cost-effective optimal times to deploy a vaccine. Our results suggest that as long as the vaccine prices are sufficiently low, vaccination should always start from or immediately after the onset of a cholera outbreak. {\textcopyright} 2014 World Scientific Publishing Company.},
  annote        = {RAYYAN-INCLUSION: {"elishaare"=>"Included"} | RAYYAN-LABELS: XP,px},
