@Article{Deelstra2003,
  author   = {Deelstra, Ytsen and Nooteboom, Sibout G. and Kohlmann, H. Ralph and {Van Den Berg}, Job and Innanen, Sally},
  journal  = {Environmental Impact Assessment Review},
  title    = {{Using knowledge for decision-making purposes in the context of large projects in The Netherlands}},
  year     = {2003},
  issn     = {01959255},
  number   = {5},
  pages    = {517--541},
  volume   = {23},
  abstract = {Policy-related research in general, and impact assessments in particular, are too loosely connected to decision-making processes. The result is often sub-optimal or even undesirable, as one of two situations arises: (1) much research is done; however, those with the real power to make decisions do not make use of all of the resulting information, or (2) advocates of contrary opinions struggle with each other, using policy-related research as ammunition. To avoid these unwanted situations, the connection between the world of knowledge and the world of decision-making should be carefully constructed, by connecting the process of decision-making to the academic research and carefully developing research goals in response to the demands of decision-makers. By making these connections in a stepwise manner, knowledge may generate new insights and views for involved decision-makers and stakeholders, thus changing perceptions and problem definitions. In this way, these actors learn about the possibilities of several alternatives as well as each other's perceptions, and thus can make educated decisions leading to the most desirable and socially acceptable solution. The way this proposed method works is illustrated using two cases in The Netherlands: the project "Mainport Rotterdam" (the enlargement of the port of Rotterdam), the project "A fifth runway for Amsterdam Airport (Schiphol)". {\textcopyright} 2003 Elsevier Science Inc. All rights reserved.},
  doi      = {10.1016/S0195-9255(03)00070-2},
  file     = {:C\:/Users/JAMESAZAM/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Deelstra et al. - 2003 - Using knowledge for decision-making purposes in the context of large projects in The Netherlands.pdf:pdf},
  keywords = {Decision-making processes, Impact assessments, Multi-actor decision-making, Negotiated knowledge, Policy-related research},
}

@Article{Wang2015,
  author        = {Wang, Qin and Zhao, Laijun and Huang, Rongbing and Yang, Youping and Wu, Jianhong},
  journal       = {Discrete and Continuous Dynamical Systems - Series B},
  title         = {{Interaction of media and disease dynamics and its impact on emerging infection management}},
  year          = {2015},
  issn          = {15313492},
  number        = {1},
  pages         = {215--230},
  volume        = {20},
  abstract      = {The 2002-2003 SARS outbreaks exhibited some distinct features such as rapid spatial spread, massive media reports, and fast self-control. These features were shared by the 2009 pandemic in uenza and will be experienced by other emerging infectious diseases. We focus on the dynamic interaction of media reports, epidemic outbreak and behavior change in the population and formulate a compartmental model, that tracks the evolution of the human population. Such population is characterized by the disease progression (sus- ceptible, infected, hospitalized, and recovered) and by the extent to which the media has impacted, so individuals have modified their behaviors to reduce their transmissibility and infectivity. The model also describes the dynamics of media reports by considering how media is in uenced by the disease statis-fltics (numbers of infected and hospitalized individuals, for example). We then conduct linear stability analysis and numerical simulations to study how inter- action of media reports and disease progress affects the disease transmission dynamics, so as to shed light on what type of media will be the most effective for the control of an epidemic.},
  doi           = {10.3934/dcdsb.2015.20.215},
  file          = {:C\:/Users/JAMESAZAM/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2015 - Interaction of media and disease dynamics and its impact on emerging infection management.pdf:pdf},
  keywords      = {Basic reproduction number, Emerging infection management, Epidemic model, James, Media impact, Stability, excluded, reviewed},
  mendeley-tags = {James,excluded,reviewed},
}

@Article{Panja2018,
  author        = {Panja, Prabir and Mondal, Shyamal Kumar and Chattopadhyay, Joydev},
  journal       = {International Journal of Nonlinear Sciences and Numerical Simulation},
