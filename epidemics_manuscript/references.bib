  keywords      = {Disease Outbreaks, Emigration and Immigration, Humans, Kansas, Models, Theoretical, United States, article, compartment model, epidemic, health hazard, human, income, infection prevention, influenza vaccine, marriage, migration, risk management, rural population, simulation, theoretical model, vaccination, xiaoxi},
  language      = {English},
  mendeley-tags = {xiaoxi},
  pmid          = {20644715},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77955399282&doi=10.1371%2Fjournal.pone.0011569&partnerID=40&md5=41b400c154bb7403539405fc728c76d3},
