  doi           = {10.1080/23312025.2019.1619219},
  keywords      = {xiaoxi},
  mendeley-tags = {xiaoxi},
}

@Article{Dorea2017,
  author        = {D{\'{o}}rea, Fernanda C. and N{\"{o}}remark, Maria and Widgren, Stefan and Fr{\"{o}}ssling, Jenny and Boklund, Anette and Halasa, Tariq and St{\aa}hl, Karl},
  journal       = {Frontiers in Veterinary Science},
