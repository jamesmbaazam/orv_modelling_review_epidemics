}

@InProceedings{Li2011,
  author        = {Li, Lu and Fa, Zhang and Cai, Hong},
  booktitle     = {ICEMMS 2011 - Proceedings: 2011 2nd IEEE International Conference on Emergency Management and Management Sciences},
