  keywords      = {Antiviral treatment, Influenza pandemic, Isolation, James, Optimal control, reviewed},
  language      = {English},
  mendeley-tags = {James,reviewed},
  pmid          = {20382168},
  url           = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-77953684619&doi=10.1016%2Fj.jtbi.2010.04.003&partnerID=40&md5=6c9e752e489e068ba3f134b102f89707     https://www.sciencedirect.com/science/article/abs/pii/S0022519310001864?via%3Dihub},
}

@InCollection{Dibble2010,
