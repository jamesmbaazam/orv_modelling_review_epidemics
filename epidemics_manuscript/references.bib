@Article{Yang2014,
  author    = {Yang, Wanfei and Parisi, Monika and Lahue, Betsy J. and Uddin, Md Jasim and Bishai, David},
  journal   = {Vaccine},
  title     = {{The budget impact of controlling wastage with smaller vials: A data driven model of session sizes in Bangladesh, India (Uttar Pradesh), Mozambique, and Uganda}},
  year      = {2014},
