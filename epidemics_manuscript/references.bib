@Article{Lee2017,
  author    = {Lee, Bruce Y. and Wedlock, Patrick T. and Haidari, Leila A. and Elder, Kate and Potet, Julien and Manring, Rachel and Connor, Diana L. and Spiker, Marie L. and Bonner, Kimberly and Rangarajan, Arjun and Hunyh, Delphine and Brown, Shawn T.},
  journal   = {Vaccine},
