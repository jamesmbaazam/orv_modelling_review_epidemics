  file          = {:C\:/Users/JAMESAZAM/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hsieh et al. - 2014 - Quantification of bird-to-bird and bird-to-human infections during 2013 novel H7N9 avian influenza outbreak in Chi.pdf:pdf},
  keywords      = {China, Disease Outbreaks, Human, Influenza, James, excluded, reviewed},
  mendeley-tags = {James,excluded,reviewed},
  pmid          = {25479054},
}

