@Article{Beest2013,
  author   = {Beest, Dennis E Te and {Van Boven}, Michiel and Hooiveld, Mari{\"{e}}tte and {Van Den Dool}, Carline and Wallinga, Jacco},
  journal  = {American Journal of Epidemiology},
  title    = {{Driving factors of influenza transmission in the netherlands}},
