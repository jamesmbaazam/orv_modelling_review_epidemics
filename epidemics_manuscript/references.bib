}

@Book{Ponnambalam2016,
  author        = {Ponnambalam, Loganathan and Xiuju, Fu and Goh, Rick Siow Mong and Sarawgi, Disha},
  title         = {{A multi-agent model for adaptive vaccination during infectious disease outbreaks}},
  year          = {2016},
  isbn          = {9781467384377},
  abstract      = {Infectious disease outbreaks are a huge burden on healthcare, causing hospitalizations, deaths and rigorously impairing the economy. In this work, an age-structured multi-agent model has been developed to simulate an epidemic spread among the US population and strategize adaptive vaccination planning to control the spread. The population was split into six distinct groups of agents depending upo on their age. In addition, the calibration of the multi-agent model parameters for H1N1 2009 pandemic, validation of the model using H1N1 2009 pandemic data from Centers for Disease Control and Prevention (CDC, US) was carried out. Using these data, the model was calibrated such that the H1N1 deaths predicted by the model was comparable to that of the deaths reported by CDC, while the H1N1 hospitalizations predicted were within the 95% confidence interval. A series of hypothetical simulations of a H1N1 like pandemic outbreak among the US population to illustrate the effectiveness of various adaptive strategies proposed in the literature will be presented. Each set of simulation was replicated 100 times so as to average the stochastic effects of parameter(s) uncertainty. The multi-agent model developed in this work can be used as a decision support system to systematically gauge the effectiveness of various interventions so as to aid healthcare policy makers to design dynamic, optimal health interventions to counter disease outbreaks.},
  annote        = {RAYYAN-INCLUSION: {"James"=>"Included", "elishaare"=>"Included", "xiaoxi.pang"=>"Included"} | RAYYAN-LABELS: XP,px},
  booktitle     = {2016 International Conference on Computing Technologies and Intelligent Data Engineering, ICCTIDE 2016},
  doi           = {10.1109/ICCTIDE.2016.7725354},
  file          = {:C\:/Users/JAMESAZAM/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ponnambalam et al. - 2016 - A Multi-Agent Model for Adaptive Vaccination during Infectious Disease Outbreaks(2).pdf:pdf},
  keywords      = {Adaptive interventions, Age-structured models, Decision support system, H1N1 Pandemic, James, Multi-agent modeling, Vaccine allocation, included},
