  eprint        = {1503.00183},
  file          = {:C\:/Users/JAMESAZAM/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bhattacharyya, Bauch, Breban - 2015 - Role of word-of-mouth for programs of voluntary vaccination A game-theoretic approach.pdf:pdf},
  keywords      = {Game theory, James, Mathematical model, Vaccination, Word of mouth, excluded, reviewed},
  mendeley-tags = {James,excluded,reviewed},
  pmid          = {26367185},
}

@Article{LonginiJr2004,
  author   = {{Longini Jr}, I M and Halloran, M E and Nizam, A and Yang, Y},
  journal  = {American Journal of Epidemiology},
  title    = {{Containing Pandemic Influenza with Antiviral Agents}},
  year     = {2004},
  issn     = {00029262 (ISSN)},
  number   = {7},
  pages    = {623--633},
  volume   = {159},
  abstract = {For the first wave of pandemic influenza or a bioterrorist influenza attack, antiviral agents would be one of the few options to contain the epidemic in the United States until adequate supplies of vaccine were available. The authors use stochastic epidemic simulations to investigate the effectiveness of targeted antiviral prophylaxis to contain influenza. In this strategy, close contacts of suspected index influenza cases take antiviral agents prophylactically. The authors compare targeted antiviral prophylaxis with vaccination strategies. They model an influenza pandemic or bioterrorist attack for an agent similar to influenza A virus (H2N2) that caused the Asian influenza pandemic of 1957-1958. In the absence of intervention, the model predicts an influenza illness attack rate of 33% of the population (95% confidence interval (CI): 30, 37) and an influenza death rate of 0.58 deaths/1,000 persons (95% CI: 0.4, 0.8). With the use of targeted antiviral prophylaxis, if 80% of the exposed persons maintained prophylaxis for up to 8 weeks, the epidemic would be contained, and the model predicts a reduction to an illness attack rate of 2% (95% CI: 0.2, 16) and a death rate of 0.04 deaths/1,000 persons (95% CI: 0.0003, 0.25). Such antiviral prophylaxis is nearly as effective as vaccinating 80% of the population. Vaccinating 80% of the children aged less than 19 years is almost as effective as vaccinating 80% of the population. Targeted antiviral prophylaxis has potential as an effective measure for containing influenza until adequate quantities of vaccine are available.},
  address  = {Department of Biostatistics, Rollins School of Public Health, Emory University, Atlanta, GA, United States Department of Biostatistics, Rollins School of Public Health, Emory University, 1518 Clifton Road, N.E., Atlanta, GA 30322, United States %[2019-01-},
  keywords = {Adolescent, Aged, Antiviral Agents, Antiviral agents, Bioterrorism, Child, Preschool, Computer simulation, Disease outbreaks, Humans, Infant, Newborn, Influenza, Influenza A virus, Influenza Vaccines, Influenza vaccine, Influenza virus A, Human, Middle Aged, Models, Theoretical, Monte Carlo method, North America, Stochastic Processes, United States, adult, antivirus agent, article, biological warfare, biological weapon, child, disease control, drug prescribing, epidemic, human, influenza vaccination, mortality, simulation, viral disease},
  language = {English},
  url      = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-1642462098&doi=10.1093%2Faje%2Fkwh092&partnerID=40&md5=de1607c8c11e42713e5857546e82e8a6},
}

@Article{Nik2017,
  author        = {Nik, H Saberi and Zarasvand, T},
  journal       = {Journal of Mathematical Extension},
  title         = {{Optimal and Adaptive Control of an Epidemic Model of Influenza with Unknown Parameters}},
  year          = {2017},
  issn          = {1735-8299},
  number        = {3},
  pages         = {103--127},
  volume        = {11},
  abstract      = {This paper deals with the nonlinear dynamics, chaos, optimal and
adaptive control of an epidemic model for H1N1 influenza with unknown
parameters. Two different control strategies are explored. First, we use
the optimal control theory to reduce the infected individuals and the
cost of vaccination. Then, we study the problem of optimal control of
unstable steady-states of H1N1 influenza system using a nonlinear
control approach. Finally, we propose the Lyapunov stability to control
of the chaotic epidemic model of influenza with unknown parameters by a
feedback control approach. Matlab bvp4c and ode45 have been used for
solving the autonomous chaotic systems and the extreme conditions
obtained from the Pontryagin's maximum principle (PMP). Furthermore,
numerical simulations are included to demonstrate the effectiveness of
the proposed control strategies.},
  annote        = {RAYYAN-INCLUSION: {"James"=>"Included"} | RAYYAN-LABELS: XP,px},
  keywords      = {Optimal control; influenza; epidemic model; lyapun, xiaoxi},
  mendeley-tags = {xiaoxi},
}

