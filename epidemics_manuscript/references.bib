@Article{Peters2015,
  author   = {Peters, Micah D.J. and Godfrey, Christina M. and Khalil, Hanan and McInerney, Patricia and Parker, Deborah and Soares, Cassia Baldini},
  journal  = {International Journal of Evidence-Based Healthcare},
